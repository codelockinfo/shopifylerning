                //   window.location.reload();
                // })
                //$btnSelectedAll.removeClass('pe_none');
                window.location.reload()
              })
            }
          })
          let getProductsId = function(url){
            $.ajax({
              type: "GET",
              url: url,
              dataType: "json",
              success: function(dataProduct, status, xhr) {
                all_prdid = all_prdid.concat(dataProduct.products);
                // if (xhr.getResponseHeader("link") !== null && xhr.getResponseHeader("link").indexOf('>; rel="next"') > 0) {
                //   url = xhr.getResponseHeader("link").split('>; rel="next"')[0].split('.myshopify.com');
                //   getProductsId(url[url.length-1]);
                // }
                // else {
                  //$btnUpdateAll.removeClass('loading').addClass('pe_none');
                  loopGetMetafeilds(all_prdid[0].id, function(){
                    count_ajax_metafields_item_all = 0;
                    $btnUpdateAll.text(productsCount_ +'/'+ all_prdid.length)
                    pushToShop(metafields_all[0], function(){
                      window.location.reload();
                    })
                  })
                // }
              }
            })
          }
          let loopGetMetafeilds = function(pid, callback){
            count_ajax_metafields_item_all = 0;
            $.ajax({
              type: "GET",
              url: '/admin/api/2021-07/products/'+pid+'/metafields.json',
              headers: {
                "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
                "x-csrf-token": csrf_token
              },
              dataType: "json",
              success: function(data, status, xhr) {
                pushToProduct(pid, data.metafields, metafields_all[count_ajax_metafields_item_all], function() {

                  productsCount_++;
                  $btnUpdateAll.text(productsCount_ +'/'+ all_prdid.length).removeClass('loading').addClass('pe_none')
                  $btnSelectedAll.text(productsCount_ +'/'+ all_prdid.length).removeClass('loading').addClass('pe_none')
                  //console.log(productsCount_)
                  if (productsCount_ < all_prdid.length) {
                    loopGetMetafeilds(all_prdid[productsCount_].id, callback);
                  }
                  else {
                    callback();
                  }
                });
              }
            })
          }
          let pushToProduct = function(pid, metafields, metafield, callback){
            let metaId = _.filter(metafields, { 'namespace': metafield.namespace, 'key': metafield.key })[0];
            let ajaxPost = function () {
              $.ajax({
                url: '/admin/api/2020-01/products/'+pid+'/metafields.json',
                type: "POST",
                headers: {
                  "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
                  "x-csrf-token": csrf_token
                },
                data: {"metafield": metafield}
              })
              .done(function(data, textStatus, xhr) {
                ajaxDone();
              })
              .fail(function() {
                $btnUpdateAll.removeClass('loading');
              });
            }
            // let ajaxPut = function (metaId) {
            //   $.ajax({
            //     url: '/admin/api/2020-01/products/'+pid+'/metafields/'+metaId+'.json',
            //     type: "PUT",
            //     headers: {
            //       "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
            //       "x-csrf-token": csrf_token
            //     },
            //     data: {"metafield": {
            //       "id": metaId,
            //       "value": metafield.value,
            //       "value_type": "string"
            //     }}
            //   })
            //   .done(function(data, textStatus, xhr) {
            //     ajaxDone();
            //   })
            //   .fail(function() {
            //     $btnUpdateAll.removeClass('loading');
            //   });
            // }
            let ajaxDelete = function (metaId) {
              $.ajax({
                url: '/admin/api/2020-01/products/'+pid+'/metafields/'+metaId+'.json',
                headers: {
                  "content-type": "application/x-www-form-urlencoded; charset=UTF-8",
                  "x-csrf-token": csrf_token
                },
                type: "DELETE"
              })
              .done(function(data, textStatus, xhr) {
                ajaxDone();
              })
              .fail(function() {
                $btnUpdateAll.removeClass('loading');
              });
            }
            let ajaxDone = function () {
              count_ajax_metafields_item_all++;
              if (count_ajax_metafields_item_all < metafields_all.length) {
                pushToProduct(pid, metafields, metafields_all[count_ajax_metafields_item_all], callback)
              } else {
                callback()
              }
            }
            if (metaId != undefined) {
              if ( $('#'+metafield.key).is(':checked') || $('[data-check="'+metafield.key+'"]').is(':checked') ) {
                if (metafield.value != '') {
                  ajaxPost();
                } else {
                  ajaxDelete(metaId.id);
                }
              } else {
                ajaxDone();
              }
            } else if (metafield.value != '') {
              ajaxPost();
            } else {
              ajaxDone();
            }
          }

        });
      })
    </script>
