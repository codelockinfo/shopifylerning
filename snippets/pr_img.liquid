{%- render 'product_labels',product:product -%}{%- assign image = product.images[0] | default:settings.placeholder_img -%}{%- assign ratio = image.aspect_ratio -%}
<a data-linkhref class="db" href="{{url_last}}">
	{%- render 'product_labels' -%}
	<div {% if image.alt contains 'imgt4_colorsolid' %}data-widths="[1]" {% endif %}class="pr_lazy_img main-img nt_img_ratio lazyload nt_bg_lz" data-id="{{image.id}}" data-bgset="{{ image | img_url: '1x1' }}" data-parent-fit="width" data-wiis data-ratio="{{ratio}}" data-sizes="auto" style="padding-top:{{ 1 | divided_by: ratio | times: 100}}%;"></div>
</a>