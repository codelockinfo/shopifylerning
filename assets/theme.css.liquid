  .lazyloaded .section-title {
   font-family: {% if fnt_df_set == '1' %}{{font-stack-body}}{% elsif fnt_df_set == '2' %}{{font-stack-heading}}{% else %}{{font-stack-sub}}{% endif %}
  }
