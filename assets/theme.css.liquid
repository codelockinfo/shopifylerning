  }
  .nt_pr h3.product-title { font-family: {% if fnt_df_pr == '1' %}{{font-stack-body}}{% elsif fnt_df_pr == '2' %}{{font-stack-heading}}{% else %}{{font-stack-sub}}{% endif %} }
  .fnt_df_blog,.widget-title, .lazyloaded .widget-title,.post_nt_loop h4 { font-family: {% if fnt_df_blog == '1' %}{{font-stack-body}}{% elsif fnt_df_blog == '2' %}{{font-stack-heading}}{% else %}{{font-stack-sub}}{% endif %} }

