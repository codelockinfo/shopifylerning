{%- liquid
  assign se_blocks = section.blocks 
  assign tp_blocks = se_blocks | map: 'type' | join: ',' | remove:',link2' | remove:',link' | remove:',pr' | remove:',banner' | remove:',html' | remove:',cat' | split: ','
  assign blocktp = 't4'
  assign admin_sp = request.design_mode
  assign arr_terms = search.terms |split:'product_type:'
  assign search_terms = arr_terms[0] | strip 
  assign search_pr_type = arr_terms[1] | strip
  assign h__bgimg = settings.h__bgimg
  assign limit = 5 -%} 

{%- if template.suffix == "header" and template.name == 'search' -%} 
  {%- render 'menu_import',section:section -%}
{%- else -%}
  <div class="header__mid{% if h__bgimg != blank %} lazyload h__lz_img" data-bgset="{{ h__bgimg | img_url: '1x1' }}" data-ratio="{{h__bgimg.aspect_ratio}}" data-sizes="auto"{% else %}"{% endif %}><div class="container">
     <div class="row al_center css_h_se">
        <div class="col-md-4 col-3 dn_lg">{%- render 'push_menu' -%}</div>
        <div class="col-lg-3 col-md-4 col-6 tc tl_lg">{%- render 'nt_logo' with 'div' -%}</div>
        <div class="col-6 dn db_lg cl_h_search{% if collection != blank and settings.show_search_suggest %} atc_opended_rs{% endif %}">
          <form action="{{ routes.search_url }}" method="get" class="h_search_frm js_frm_search pr" role="search">
               <input type="hidden" name="type" value="product">
               <input type="hidden" name="options[unavailable_products]" value="{{settings.unavailable_prs}}">
               <input type="hidden" name="options[prefix]" value="{{settings.prefix}}">
               <div class="row no-gutters al_center">
                 {%- if settings.filter_type_search and shop.types.size > 0 -%}
                    <div class="frm_search_cat col-auto">
                        <select name="product_type">
                          <option value="*">{{ 'general.search.all_cat' | t }}</option>
                          {%- for product_type in shop.types -%}{%- if product_type == blank %}{% continue-%}{% endif -%}<option value="{{product_type}}"{% if search_pr_type == product_type %} selected="selected"{% endif %}>{{product_type}}</option>{%- endfor -%}
                        </select>
                    </div>
                    <div class="col-auto h_space_search"></div>
                  {%- endif -%}
                 <div class="frm_search_input pr oh col">
                   <input class="h_search_ip js_iput_search" autocomplete="off" type="text" name="q" placeholder="{{ 'general.search.placeholder2' | t }}" value="{% if template.suffix == blank and search.performed %}{{search_terms |remove:'*'}}{% endif %}">
                 </div>
                 <div class="frm_search_cat col-auto">
                  <button class="h_search_btn js_btn_search" type="submit">{{ 'general.search.btn' | t }}</button>
                 </div>
               </div>
             </form>
             <div class="pr">
                  <div class="mini_cart_content fixcl-scroll widget">
                     <div class="fixcl-scroll-content product_list_widget">
                        <div class="ld_bar_search"></div>
                        <div class="skeleton_wrap skeleton_js dn">
                           {%- for i in (1..4) -%}
                           <div class="row mb__10 pb__10">
                              <div class="col-auto widget_img_pr"><div class="skeleton_img"></div></div>
                              <div class="col widget_if_pr"><div class="skeleton_txt1"></div><div class="skeleton_txt2"></div></div>
                           </div>
                           {%- endfor -%}
                        </div>
                        <div class="js_prs_search">
                        {%- if collection != blank and settings.show_search_suggest -%}
                            {%- liquid
                             assign on_sale_txt = 'products.product.on_sale' | t
                             assign save_js = 'products.product.save_js' | t: saved_amount:'saved_amount' -%}
                           {%- for product in collection.products limit:limit -%}{%- render 'pr_group_loop',product:product,c:collection,on_sale_txt:on_sale_txt,save_js:save_js -%}{%- endfor -%}
                           {%- if collection.all_products_count > limit -%}<a href="{{collection.url}}" class="btn fwsb detail_link">{{ 'general.search.view_all' | t }} <i class="facl facl-right"></i></a>{%- endif -%}
                        {%- endif -%}
                        </div>
                     </div>
                  </div>
             </div>
        </div>
        <div class="col-lg-3 col-md-4 col-3 tr">{%- render 'nt_group_btns' -%}</div>
     </div>
  </div></div> 
  <div class="header__bot border_{{section.settings.border}} dn db_lg"><div class="container">
   <div class="row no-gutters al_center">
      {%- if section.settings.shop_cat -%}
      <div class="col-auto ha8_cat">
         <h5 class="mg__0 fs__14 flex al_center"><i class="las la-bars mr__5 fs__18"></i><span class="dib truncate">{{section.settings.txt_cat}}</span></h5>
         <div class="h_cat_nav pa op__0"><ul class="lazy_menu lazyload lazy_h_cat" data-include="{{routes.search_url}}?view=hcat_nav"><li class="lds_bginfinity pr"></li></ul></div>
      </div>
      {%- endif -%}
      <div class="col ha8_nav">{%- render 'menu_inc',admin_sp:admin_sp,se_blocks:se_blocks,tp_blocks:tp_blocks -%}</div>
      {%- if section.settings.text != blank %}<div class="col-3 fs__12 ha8_txt tr">{{section.settings.text}}</div>{% endif -%}
   </div>
    
  </div></div>

  {%- if admin_sp -%}
      {%- assign arr_sub = 'link,link2,banner,html,pr,cat' -%}
      {%- for block in se_blocks %}<div {% if arr_sub contains block.type %}{% assign id_data = block.id %} {{ block.shopify_attributes }}{% else %}{% assign id_data2 = block.id %}{% endif %}id="bkjs_{{id_data}}" data-id='{{id_data2}}'></div>{% endfor -%}
  {%- endif -%}
   {%- style -%} 
      .ha8_cat h5 {
          background-color: {{section.settings.bgcat}};
          color: {{section.settings.clcat}};
      }
      .header__mid .css_h_se {min-height: {{section.settings.height}}px}
      .header__bot,
      .header__mid .nt_menu{min-height: {{section.settings.height_2}}px}
      .ha8_cat h5,.nt_menu >li>a{height:{{section.settings.height_2}}px}
      .header__bot.border_true {border-color:{{section.settings.color}}}
      .header__bot {
       background-color: {{section.settings.bg}};
      }
      {%- assign clnav = section.settings.cl_txt -%}
      .header__bot .ha8_nav .nt_menu>li>a, .header__bot .ha8_txt, .header__bot .ha8_txt a {
        color: {{clnav}} !important;
      }
      .header__bot .ha8_nav .nt_menu>li>a>i {
        color: {{ clnav | color_modify: 'alpha', 0.6 }} !important;
      }
       .header__bot .ha8_nav .nt_menu>li.has-children>a:after {
        color: {{ clnav | color_modify: 'alpha', 0.45 }} !important;
      }
   {%- endstyle -%}
{%- endif -%}

